{
  "name": "@hashgraph/asset-tokenization-contracts",
  "version": "1.6.1",
  "main": "./build/typechain-types/index.js",
  "module": "./build/typechain-types/index.js",
  "files": [
    "build/"
  ],
  "exports": {
    ".": {
      "import": "./build/typechain-types/index.js",
      "require": "./build/typechain-types/index.js"
    },
    "./typechain-types/index.js": {
      "import": "./build/typechain-types/index.js",
      "require": "./build/typechain-types/index.js"
    }
  },
  "scripts": {
    "typechain": "npx hardhat typechain",
    "compile": "npx hardhat compile",
    "compile:force": "npx hardhat compile --force && npm run build",
    "test-coverage": "npx hardhat coverage --testfiles 'test/unitTests/**/*.ts'",
    "test": "npx hardhat test test/unitTests/**/*.ts test/unitTests/*/**/*.ts test/unitTests/*/*/**/*.ts",
    "test:demo": "npx hardhat test test/demo/Demo.test.ts",
    "test:demo:Hedera": "npx hardhat test test/demo/Demo.test.Hedera.ts",
    "test:factory": "npx hardhat test test/unitTests/factory/factory.test.ts",
    "test:resolverProxy": "npx hardhat test test/unitTests/resolverProxy/resolverProxy.test.ts",
    "test:resolver": "npx hardhat test test/unitTests/resolver/BusinessLogicResolver.test.ts",
    "test:accessControl": "npx hardhat test test/unitTests/layer_1/accessControl/accessControl.test.ts",
    "test:controlList": "npx hardhat test test/unitTests/layer_1/controlList/controlList.test.ts",
    "test:pause": "npx hardhat test test/unitTests/layer_1/pause/pause.test.ts",
    "test:corporateActions": "npx hardhat test test/unitTests/layer_2/corporateActions/corporateActions.test.ts",
    "test:erc1643": "npx hardhat test test/unitTests/layer_1/ERC1400/ERC1643/erc1643.test.ts",
    "test:erc1644": "npx hardhat test test/unitTests/layer_1/ERC1400/ERC1644/erc1644.test.ts",
    "test:erc1594": "npx hardhat test test/unitTests/layer_1/ERC1400/ERC1594/erc1594.test.ts",
    "test:erc1410": "npx hardhat test test/unitTests/layer_2/ERC1400/ERC1410/erc1410.test.ts",
    "test:erc20": "npx hardhat test test/unitTests/layer_1/ERC1400/ERC20/erc20.test.ts",
    "test:snapshots": "npx hardhat test test/unitTests/layer_1/snapshots/snapshots.test.ts",
    "test:equity": "npx hardhat test test/unitTests/layer_2/equity/equity.test.ts",
    "test:bond": "npx hardhat test test/unitTests/layer_2/bond/bond.test.ts",
    "test:scheduledSnapshots": "npx hardhat test test/unitTests/layer_2/scheduledSnapshots/scheduledSnapshots.test.ts",
    "test:cap": "npx hardhat test test/unitTests/layer_1/cap/cap.test.ts",
    "test:securityUSA": "npx hardhat test test/unitTests/layer_3/securityUSA/securityUSA.test.ts",
    "test:lock": "npx hardhat test test/unitTests/layer_1/lock/lock.test.ts",
    "test:diamondCutManager": "npx hardhat test test/unitTests/resolver/diamondCutManager.test.ts",
    "size": "npx hardhat size-contracts",
    "build": "rimraf build && npx tsc -p tsconfig.json",
    "keccak256": "npx hardhat keccak256",
    "keccak": "npm run keccak256",
    "hash": "npm run keccak256",
    "lint:sol": "solhint 'contracts/**/*.sol'",
    "lint:ts": "npx eslint . --ext .ts",
    "lint": "npm run lint:sol && npm run lint:ts",
    "prettier": "prettier --write  \"**/*.{ts,js,sol}\"",
    "doc": "npx hardhat dodoc",
    "slither:analysis": "docker run -t --rm -v /var/run/docker.sock:/var/run/docker.sock -v \"$(pwd)\":\"/home/ethsec/contracts\" -w \"/home/ethsec/contracts\" -u 0:0 trailofbits/eth-security-toolbox -c \"solc-select install 0.8.18  && solc-select use 0.8.18  && slither ./contracts --solc-remaps @=node_modules/@ &> ./Slither/contract-analysis.txt\"",
    "slither:storageLayout": "docker run -t --rm -v /var/run/docker.sock:/var/run/docker.sock -v \"$(pwd)\":\"/home/ethsec/contracts\" -w \"/home/ethsec/contracts\" -u 0:0 trailofbits/eth-security-toolbox -c \"solc-select install 0.8.18  && solc-select use 0.8.18  && slither ./contracts --print variable-order --solc-remaps @=node_modules/@ &> ./Slither/contract-storage-layout.txt\"",
    "slither:inheritance": "docker run -t --rm -v /var/run/docker.sock:/var/run/docker.sock -v \"$(pwd)\":\"/home/ethsec/contracts\" -w \"/home/ethsec/contracts\" -u 0:0 trailofbits/eth-security-toolbox -c \"solc-select install 0.8.18  && solc-select use 0.8.18  && slither ./contracts --print inheritance --solc-remaps @=node_modules/@ &> ./Slither/contract-inheritance.txt\"",
    "slither:summary": "docker run -t --rm -v /var/run/docker.sock:/var/run/docker.sock -v \"$(pwd)\":\"/home/ethsec/contracts\" -w \"/home/ethsec/contracts\" -u 0:0 trailofbits/eth-security-toolbox -c \"solc-select install 0.8.18  && solc-select use 0.8.18  && slither ./contracts --print human-summary --solc-remaps @=node_modules/@ &> ./Slither/contract-summary.txt\"",
    "slither": "npm run slither:summary && npm run slither:storageLayout && npm run slither:inheritance && npm run slither:analysis",
    "pre-commit": "npm run prettier && npm run lint",
    "prettierCheck": "prettier --check",
    "prepack": "npm run compile:force",
    "cleanCache": "npx hardhat clean"
  },
  "keywords": [],
  "author": "",
  "license": "Apache-2.0",
  "devDependencies": {
    "@hashgraph/hardhat-hethers": "^1.0.4",
    "@hashgraph/sdk": "^2.23.0",
    "@nomicfoundation/hardhat-chai-matchers": "^1.0.3",
    "@nomicfoundation/hardhat-network-helpers": "^1.0.6",
    "@nomicfoundation/hardhat-toolbox": "^2.0.2",
    "@nomiclabs/hardhat-ethers": "2.1.1",
    "@nomiclabs/hardhat-etherscan": "3.1.0",
    "@nomiclabs/hardhat-waffle": "^2.0.6",
    "@openzeppelin/contracts": "^4.8.2",
    "@openzeppelin/contracts-upgradeable": "^4.8.2",
    "@openzeppelin/hardhat-upgrades": "^1.22.1",
    "@primitivefi/hardhat-dodoc": "^0.2.3",
    "@typechain/ethers-v5": "10.1.0",
    "@typechain/hardhat": "6.1.3",
    "@types/jest": "^29.5.7",
    "@typescript-eslint/eslint-plugin": "^5.38.1",
    "@typescript-eslint/parser": "^5.38.1",
    "chai": "^4.3.6",
    "chai-as-promised": "^7.1.1",
    "dotenv": "^16.0.3",
    "eslint": "^8.24.0",
    "ethers": "5.7.2",
    "hardhat": "^2.20.1",
    "hardhat-abi-exporter": "^2.10.0",
    "hardhat-contract-sizer": "^2.6.1",
    "hardhat-gas-reporter": "^1.0.9",
    "prettier": "^2.8.2",
    "prettier-plugin-solidity": "^1.1.1",
    "solhint": "^3.3.7",
    "solhint-plugin-prettier": "^0.0.5",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typechain": "8.1.0",
    "typescript": "^5.2.2",
    "web3": "^4.2.2"
  },
  "dependencies": {
    "solidity-coverage": "^0.8.12"
  }
}
